#init 2

{
	x := 1
	y := 2
	if x == 1:
		x := y
	else:
		x := 10
	print(x)			// 10
}

MAIN:
	ipush 1				// 
	set [0]				// x := 1
	ipush 2				//
	set [1]				// y := 2
	get [0]				//
	ipush 1				//
	eqi					// x == 1
	jumpz THEN ELSE		//

THEN:
	get [1]				//
	set [0]				// x := y
	jump CONT			//
  
ELSE:
	ipush 10			//
	set [0]				// x: = 10
	jump CONT			//
 
CONT:
	get [0]				//
	print				// print(x)
						//
	ipush 0				// load exitcode 0
	return				// exit