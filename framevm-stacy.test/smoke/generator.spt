module smoke_generator

language framevm-stacy

test Generator example RISC [[MAIN:
	new
	newscope P FOR_INIT
	
FOR_INIT:
	new 0
	dup
	link [] P

	new 0
	dup
	link [] P

	try FOR_START FOR_TERM FOR_END
	
FOR_START:
	new 0
	dup
	link [] P

	cnew FUNC 2
	dup
	cget [x]
	csetr [x]
	dup
	cget []
	csetr [c]

	jump FOR
	
FOR:
	ccall FOR_RET
	
FOR_RET:
	rget 2
	print
	jump FOR
	
FOR_TERM:
	cret [n]
	
FOR_END:
	jump MAIN_END
	
FUNC:
	cget []
	ipush 1
	cget [c]
	transfer 2
	cget [c]
	ccall FUNC_1
	
FUNC_1:
	ipush 2
	yield FUNC_2
	
FUNC_2:
	ipush 3
	yield FUNC_3
	
FUNC_3:	
	ipush -1
	throw
	
MAIN_END:
	ipush 0
	return
]] transform "Run -> RISC" to "IntV(1)
IntV(2)
IntV(3)"


