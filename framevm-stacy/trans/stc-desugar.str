module stc-desugar

imports
	signatures/framevm-stacy-sig
	desugar/toRISC
	desugar/fixSeq
	desugar/pseudo
	desugar/register

signature
  sorts S
  constructors    
    Bind : STRING * INT -> S
    STC_Seq : [Instr] -> S
    STC_Block : STC_Label * STC_Seq * INT -> S
    STC_Program: INT * [STC_Block] -> S
    
rules  
    desugar-full = desugar; to-RISC-all
    desugar = apply-aliasses; fix-seq; desugar-pseudo-all
  	 
	resugar = resugar-seq; resugar-pseudo-all; resugar-aliasses 
   
