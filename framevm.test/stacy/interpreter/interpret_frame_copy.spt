module interpreter_frame_copy

language framevm

fixture [[#init 2
MAIN:
	[[...]]
	print
	
	ipush 0
	return
]]

test CISC shallow frame copy 1 [[
	new 1
	dup
	ipush 12
	setr [0]
	dup
	set [0]
	
	copyr shallow
	set [1]
	
	ipush 42
	set [0, 0]

	get [0, 0]
	print
	get [1, 0]
]] transform "Run -> CISC" to "IntV(42)
IntV(42)"

