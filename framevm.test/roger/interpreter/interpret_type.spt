module rgr_interpreter_type

language framevm

fixture [[#registers 99
MAIN:
	[[...]]
	print(r0)
	return(iload(0))
]]

test int =? int [[
	r0 <- int?(iload(1))
]] transform "Run -> Run" to "IntV(1)"

test frame =? int [[
	r0 <- int?(new(1))
]] transform "Run -> Run" to "IntV(0)"

test cont =? int [[
	r0 <- int?(newC(new(), MAIN, 1))
]] transform "Run -> Run" to "IntV(0)"

test cf =? int [[
