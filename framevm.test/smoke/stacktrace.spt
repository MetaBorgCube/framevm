module smoke_exception_trace

language framevm

test Exception trace example RISC [[MAIN:
	new
	dup
	link [] P
	cnew MAIN_TRY 3
	new 1
	dup
	link [] P
	cnew MAIN_CATCH 3
	try MAIN_END
	
MAIN_TRY:
	new
	dup
	link [P] P
	call FUNC1 MAIN_END
	
FUNC1:
	new
	dup
	link [] P
	call FUNC2 FUNC1_END
	
FUNC1_END:
	rget
	return
	
FUNC2:
	new 2
	dup
	get []
	setr [0]
	dup
	ipush 1
	setr [1]
	throw
	
MAIN_CATCH:
	rget
	dup
	getr [1]
	print
	getr [0]
	dup
	set [0]
	dup
	print
	dup
	getr [P]
	print
	getr [P, P]
	print
	
	cret [n]
	
MAIN_END:
	ipush 0
	return
]] transform "Run -> RISC" to "1
frame_4
frame_3
frame_0"

